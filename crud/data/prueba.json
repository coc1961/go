{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "id",
        "name",
        "amount",
        "age",
        "creationDate",
        "hijo",
        "soyArray",
        "arrayOnject"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
                "xy23"
            ],
            "pattern": "^(.*)$"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
                "Nombre"
            ],
            "pattern": "^(.*)$"
        },
        "amount": {
            "$id": "#/properties/amount",
            "type": "number",
            "title": "The Amount Schema",
            "default": 0.0,
            "examples": [
                100.45
            ]
        },
        "age": {
            "$id": "#/properties/age",
            "type": "integer",
            "title": "The Age Schema",
            "default": 0,
            "examples": [
                55
            ]
        },
        "creationDate": {
            "$id": "#/properties/creationDate",
            "type": "string",
            "title": "The Creationdate Schema",
            "default": "",
            "examples": [
                "2018-11-24T01:10:22Z"
            ],
            "pattern": "^(.*)$"
        },
        "hijo": {
            "$id": "#/properties/hijo",
            "type": "object",
            "title": "The Hijo Schema",
            "required": [
                "idAtt",
                "nameAtt"
            ],
            "properties": {
                "idAtt": {
                    "$id": "#/properties/hijo/properties/idAtt",
                    "type": "string",
                    "title": "The Idatt Schema",
                    "default": "",
                    "examples": [
                        "cc23"
                    ],
                    "pattern": "^(.*)$"
                },
                "nameAtt": {
                    "$id": "#/properties/hijo/properties/nameAtt",
                    "type": "string",
                    "title": "The Nameatt Schema",
                    "default": "",
                    "examples": [
                        "AttrHijo"
                    ],
                    "pattern": "^(.*)$"
                }
            }
        },
        "soyArray": {
            "$id": "#/properties/soyArray",
            "type": "array",
            "title": "The Soyarray Schema",
            "items": {
                "$id": "#/properties/soyArray/items",
                "type": "string",
                "title": "The Items Schema",
                "default": "",
                "examples": [
                    "elem1",
                    "elem2"
                ],
                "pattern": "^(.*)$"
            }
        },
        "arrayOnject": {
            "$id": "#/properties/arrayOnject",
            "type": "array",
            "title": "The Arrayonject Schema",
            "items": {
                "$id": "#/properties/arrayOnject/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "id",
                    "name"
                ],
                "properties": {
                    "id": {
                        "$id": "#/properties/arrayOnject/items/properties/id",
                        "type": "integer",
                        "title": "The Id Schema",
                        "default": 0,
                        "examples": [
                            123
                        ]
                    },
                    "name": {
                        "$id": "#/properties/arrayOnject/items/properties/name",
                        "type": "string",
                        "title": "The Name Schema",
                        "default": "",
                        "examples": [
                            "Nombre1"
                        ],
                        "pattern": "^(.*)$"
                    }
                }
            }
        }
    }
}